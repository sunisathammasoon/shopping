<aura:component>
    <aura:attribute name="products" type="Object[]"/>
    <aura:attribute name="total" type="integer" default="1"/>
 

    <aura:iteration items="{!v.products}" var="product">   
       <c:productlistitem product ="{!product}"></c:productlistitem>
       
    </aura:iteration>

</aura:component>	
